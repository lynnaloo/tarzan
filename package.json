    "babel-jest": "^5.3.0",
    "babelify": "^6.1.0",
    "body-parser": "1.0.2",
    "bower": "^1.3.12",
    "browserify": "^10.2.6",
    "grunt": "^0.4.5",
    "grunt-browserify": "^3.8.0",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-watch": "^0.6.1",
